<config>

    <modules>
        <Yireo_EmailOverride>
            <version>0.2.16</version>
        </Yireo_EmailOverride>
    </modules>

    <global>
        <helpers>
            <emailoverride>
                <class>Yireo_EmailOverride_Helper</class>
            </emailoverride>
        </helpers>
        <template>
            <email>
                <contacts_email_email_template translate="label" module="contacts">
                    <type>html</type>
                </contacts_email_email_template>
            </email>
        </template>
        <models>
            <emailoverride>
                <class>Yireo_EmailOverride_Model</class>
            </emailoverride>
            <core>
                <rewrite>
                    <translate>Yireo_EmailOverride_Model_Translate</translate>
                </rewrite>
                <rewrite>
                    <email_template>Yireo_EmailOverride_Model_Email_Template</email_template>
                </rewrite>
            </core>
        </models>
	</global>
</config>
